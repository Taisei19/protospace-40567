<div class="card">
  <%= link_to image_tag(rails_blob_path(prototype.image, disposition: "attachment"), class: "card__img"), prototype_path(prototype) %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype), class: "card__title" %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.user_name}", user_path(prototype.user), class: "card__user" %>
  </div>
</div>
